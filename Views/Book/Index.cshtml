@model List<BooksEntity> 

@{
    ViewData["Title"] = "Books Page";
}

<h1>Book List</h1>

<table style="border: aqua;">
    <th>
        <td>
            Id
        </td>
        <td>
            Title
        </td>
        <td>
            Author
        </td>
        <td>
            ISBN
        </td>
    </th>
    @foreach(var book in Model){
        <tr>
            <td>@book.Id</td>
            <td>@book.Title</td>
            <td>@book.Author</td>
            <td>@book.ISBN</td>
        </tr>
    }
</table>